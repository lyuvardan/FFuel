<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:tag="llShipmentDetAWBBlock"
    android:layout_height="wrap_content"
    android:layout_width="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/shipment_detail_block_layout"
        android:padding="25dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:textColor="#afb1b5"
                android:text="Flight Detail"
                android:fontFamily="sans-serif-condensed"
                android:layout_marginRight="5dp"
                android:gravity="end"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAllCaps="true"
                android:textColor="#111"
                android:textStyle="bold"
                android:tag="flightDetail"
                android:fontFamily="sans-serif-condensed"
                android:layout_marginLeft="5dp"
                android:gravity="start"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:textColor="#afb1b5"
                android:text="Airline"
                android:fontFamily="sans-serif-condensed"

                android:layout_marginRight="5dp"
                android:gravity="end"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAllCaps="true"
                android:textColor="#111"
                android:textStyle="bold"
                android:tag="airline"
                android:fontFamily="sans-serif-condensed"

                android:layout_marginLeft="5dp"
                android:gravity="start"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:textColor="#afb1b5"
                android:text="Flight"
                android:fontFamily="sans-serif-condensed"

                android:layout_marginRight="5dp"
                android:gravity="end"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAllCaps="true"
                android:textColor="#111"
                android:textStyle="bold"
                android:tag="flight"
                android:fontFamily="sans-serif-condensed"

                android:layout_marginLeft="5dp"
                android:gravity="start"/>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:textColor="#afb1b5"
                android:text="Origin"
                android:fontFamily="sans-serif-condensed"

                android:layout_marginRight="5dp"
                android:gravity="end"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAllCaps="true"
                android:textColor="#111"
                android:textStyle="bold"
                android:tag="origin"
                android:fontFamily="sans-serif-condensed"

                android:layout_marginLeft="5dp"
                android:gravity="start"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:textColor="#afb1b5"
                android:text="Destination"
                android:fontFamily="sans-serif-condensed"

                android:layout_marginRight="5dp"
                android:gravity="end"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAllCaps="true"
                android:textColor="#111"
                android:textStyle="bold"
                android:tag="destination"
                android:fontFamily="sans-serif-condensed"

                android:layout_marginLeft="5dp"
                android:gravity="start"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:textColor="#afb1b5"
                android:text="Departure Scheduled"
                android:fontFamily="sans-serif-condensed"

                android:layout_marginRight="5dp"
                android:gravity="end"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAllCaps="true"
                android:textColor="#111"
                android:textStyle="bold"
                android:tag="depScheduled"
                android:fontFamily="sans-serif-condensed"

                android:layout_marginLeft="5dp"
                android:gravity="start"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:textColor="#afb1b5"
                android:text="Departure Actual"
                android:fontFamily="sans-serif-condensed"

                android:layout_marginRight="5dp"
                android:gravity="end"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAllCaps="true"
                android:textColor="#111"
                android:textStyle="bold"
                android:tag="depActual"
                android:fontFamily="sans-serif-condensed"

                android:layout_marginLeft="5dp"
                android:gravity="start"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:textColor="#afb1b5"
                android:text="Arrival Scheduled"
                android:fontFamily="sans-serif-condensed"

                android:layout_marginRight="5dp"
                android:gravity="end"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAllCaps="true"
                android:textColor="#111"
                android:textStyle="bold"
                android:tag="arrScheduled"
                android:fontFamily="sans-serif-condensed"

                android:layout_marginLeft="5dp"
                android:gravity="start"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:textColor="#afb1b5"
                android:text="Arrival Actual"
                android:fontFamily="sans-serif-condensed"

                android:layout_marginRight="5dp"
                android:gravity="end"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAllCaps="true"
                android:textColor="#111"
                android:textStyle="bold"
                android:tag="arrActual"
                android:fontFamily="sans-serif-condensed"

                android:layout_marginLeft="5dp"
                android:gravity="start"/>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>