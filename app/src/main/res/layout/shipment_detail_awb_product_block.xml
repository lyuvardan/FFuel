<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:tag="llShipmentDetAWBProductBlock"
    android:layout_height="wrap_content"
    android:layout_width="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/shipment_detail_block_layout"
        android:padding="25dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:textColor="#afb1b5"
                android:text="Producer"
                android:fontFamily="sans-serif-condensed"
                android:layout_marginRight="5dp"
                android:gravity="end"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAllCaps="true"
                android:textColor="#111"
                android:textStyle="bold"
                android:tag="producer"
                android:fontFamily="sans-serif-condensed"
                android:layout_marginLeft="5dp"
                android:gravity="start"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:textColor="#afb1b5"
                android:text="Product"
                android:fontFamily="sans-serif-condensed"

                android:layout_marginRight="5dp"
                android:gravity="end"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAllCaps="true"
                android:textColor="#111"
                android:textStyle="bold"
                android:tag="product"
                android:fontFamily="sans-serif-condensed"

                android:layout_marginLeft="5dp"
                android:gravity="start"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:textColor="#afb1b5"
                android:text="Variety"
                android:fontFamily="sans-serif-condensed"

                android:layout_marginRight="5dp"
                android:gravity="end"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAllCaps="true"
                android:textColor="#111"
                android:textStyle="bold"
                android:tag="variety"
                android:fontFamily="sans-serif-condensed"

                android:layout_marginLeft="5dp"
                android:gravity="start"/>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:textColor="#afb1b5"
                android:text="Label"
                android:fontFamily="sans-serif-condensed"
                android:layout_marginRight="5dp"
                android:gravity="end"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAllCaps="true"
                android:textColor="#111"
                android:textStyle="bold"
                android:tag="label"
                android:fontFamily="sans-serif-condensed"
                android:layout_marginLeft="5dp"
                android:gravity="start"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:textColor="#afb1b5"
                android:text="Size"
                android:fontFamily="sans-serif-condensed"

                android:layout_marginRight="5dp"
                android:gravity="end"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAllCaps="true"
                android:textColor="#111"
                android:textStyle="bold"
                android:tag="size"
                android:fontFamily="sans-serif-condensed"

                android:layout_marginLeft="5dp"
                android:gravity="start"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:textColor="#afb1b5"
                android:text="Exp Qty"
                android:fontFamily="sans-serif-condensed"

                android:layout_marginRight="5dp"
                android:gravity="end"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAllCaps="true"
                android:textColor="#111"
                android:textStyle="bold"
                android:tag="expQty"
                android:fontFamily="sans-serif-condensed"

                android:layout_marginLeft="5dp"
                android:gravity="start"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:textColor="#afb1b5"
                android:text="Assigned Qty"
                android:fontFamily="sans-serif-condensed"

                android:layout_marginRight="5dp"
                android:gravity="end"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAllCaps="true"
                android:textColor="#111"
                android:textStyle="bold"
                android:tag="assignedQty"
                android:fontFamily="sans-serif-condensed"

                android:layout_marginLeft="5dp"
                android:gravity="start"/>
        </LinearLayout>

    </LinearLayout>

</LinearLayout>